    "## Masking"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 54,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "which values < 3?  [ True  True False False False]\n",
      "which values <= 3?  [ True  True  True False False]\n",
      "how many are < 3? 2\n",
      "are there any > 3? True\n",
      "are there any < 0? False\n",
      "are all < 10? True\n"
     ]
    }
   ],
   "source": [
    "# asking questions about data\n",
    "x = np.array([1, 2, 3, 4, 5])\n",
    "\n",
    "print(\"which values < 3? \",(x < 3))  # less than\n",
    "print(\"which values <= 3? \",(x <= 3))  # less than or equal\n",
    "print(\"how many are < 3?\",np.count_nonzero(x < 3))\n",
    "print(\"are there any > 3?\",np.any(x>3))\n",
    "print(\"are there any < 0?\",np.any(x<0))\n",
    "print(\"are all < 10?\",np.all(x < 10))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 55,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "3"
      ]
     },
     "execution_count": 55,
